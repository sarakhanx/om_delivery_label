<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_print_confirm_wizard_form" model="ir.ui.view">
        <field name="name">print.confirm.wizard.form</field>
        <field name="model">print.confirm.wizard</field>
        <field name="arch" type="xml">
            <form string="ยืนยันการพิมพ์ซ้ำ">
                <field name="message" widget="text" class="oe_read_only" style="font-size: 16px;"/>
                <footer>
                    <button name="action_confirm" string="พิมพ์ซ้ำ" type="object" class="btn-primary"/>
                    <button string="ยกเลิก" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo> 